generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model enrollment {
  id         BigInt    @id @default(autoincrement())
  created_at DateTime  @default(now()) @db.Timestamptz(6)
  title      String?   @db.VarChar
  image_url  String?
  date       DateTime? @db.Date
  body       String?
  image_path String?
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model fasilitas {
  id           BigInt   @id @default(autoincrement())
  title        String?
  image_urls   String?
  created_at   DateTime @default(now()) @db.Timestamptz(6)
  nama_sekolah String
  storage_path String?
  order        Int?     @db.SmallInt
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model hero_sliders {
  id                BigInt   @id @default(autoincrement())
  created_at        DateTime @default(now()) @db.Timestamptz(6)
  image_urls        String?
  order             Int?
  header_text       String?
  href_text         String?
  mobile_image_urls String?
  tablet_image_urls String?
  button_text       String?
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model kurikulum {
  id         BigInt   @id(map: "kurkulum_pkey") @default(autoincrement())
  created_at DateTime @default(now()) @db.Timestamptz(6)
  title      String?  @db.VarChar
  body       String?
  order      Int?     @db.SmallInt
  preview    String?
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model news {
  id            BigInt    @id @default(autoincrement())
  created_at    DateTime  @default(now()) @db.Timestamptz(6)
  title         String    @db.VarChar
  date          DateTime  @db.Date
  image_urls    String[]
  body          String
  embed         String?
  from          news_from
  storage_paths String[]
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model primary_schools {
  id           BigInt   @id @default(autoincrement())
  created_at   DateTime @default(now()) @db.Timestamptz(6)
  nama_sekolah String?
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model search_static {
  id    String @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  title String
  url   String
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model user_roles {
  id         BigInt     @default(autoincrement())
  created_at DateTime   @default(now()) @db.Timestamptz(6)
  user_id    String     @default(dbgenerated("auth.uid()")) @db.Uuid
  roles      role_enum?

  @@id([id, user_id])
}

enum news_from {
  TK_Kemurnian     @map("TK Kemurnian")
  SD_Kemurnian     @map("SD Kemurnian")
  SMP_Kemurnian    @map("SMP Kemurnian")
  Kemurnian
  TK_Kemurnian_II  @map("TK Kemurnian II")
  SD_Kemurnian_II  @map("SD Kemurnian II")
  SMP_Kemurnian_II @map("SMP Kemurnian II")
  SMA_Kemurnian_II @map("SMA Kemurnian II")
  Kemurnian_II     @map("Kemurnian II")
  TK_Kemurnian_III @map("TK Kemurnian III")
  SD_Kemurnian_III @map("SD Kemurnian III")
  Kemurnian_III    @map("Kemurnian III")
  General
}

enum role_enum {
  TK_Kemurnian     @map("TK Kemurnian")
  SD_Kemurnian     @map("SD Kemurnian")
  SMP_Kemurnian    @map("SMP Kemurnian")
  Kemurnian
  TK_Kemurnian_II  @map("TK Kemurnian II")
  SD_Kemurnian_II  @map("SD Kemurnian II")
  SMP_Kemurnian_II @map("SMP Kemurnian II")
  SMA_Kemurnian_II @map("SMA Kemurnian II")
  Kemurnian_II     @map("Kemurnian II")
  TK_Kemurnian_III @map("TK Kemurnian III")
  SD_Kemurnian_III @map("SD Kemurnian III")
  Kemurnian_III    @map("Kemurnian III")
  Admin
  Editor
}
